pipeline {
    agent any

    stages {
        stage('Git clone') {
            steps {
                echo 'Code is cloned from Github successful'
            }
        }
        
    
        stage('Build') {
            steps {
                echo 'Building is successful'
            }
        }
        
        
        stage('Testing') {
            steps {
                echo 'Testing is successful'
            }
        }
        
        
        stage('Deployment') {
            steps {
                echo 'Deployment is successful'
            }
        }
    }
}
